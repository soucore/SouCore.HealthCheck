<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFrameworks>netcoreapp3.1;net5.0;net6.0;net8.0</TargetFrameworks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<Title>HealthCheck Kafka Worker</Title>
	<Copyright>ViaHub - 2022</Copyright>
	<RepositoryUrl>https://github.com/viavarejo-internal/Via.HealthCheck/tree/master/src/HealthCheck.Kafka</RepositoryUrl>
	<Version>$(Version)</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Confluent.Kafka" Version="1.9.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Soucore.HealthCheck\Soucore.HealthCheck.csproj" />
  </ItemGroup>

</Project>
